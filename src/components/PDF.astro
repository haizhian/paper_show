---
interface Props {
  source: string;
}

const { source } = Astro.props;
---

<div class="w-full overflow-hidden">
  <iframe 
    src={source} 
    width="100%" 
    height="600" 
    class="max-w-full border-none rounded"
    style="min-height: 600px; transform: scale(1.2); transform-origin: top center;"
  ></iframe>
</div>